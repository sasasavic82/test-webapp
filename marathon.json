{
    "container": {
        "docker": {
            "image": "sash/test-webapp:latest",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "hostPort": 11223,
                    "containerPort": 8088,
                    "protocol": "tcp",
                    "servicePort": 2006
                }
            ]
        },
        "type": "DOCKER"
    },
    "cpus": 0.1,
    "healthChecks": [
        {
            "gracePeriodSeconds": 600,
            "intervalSeconds": 30,
            "maxConsecutiveFailures": 2,
            "portIndex": 0,
            "protocol": "TCP",
            "timeoutSeconds": 10
        }
    ],
    "id": "microservice-web",
    "instances": 1,
    "labels": {
        "HAPROXY_GROUP": "external",
        "HAPROXY_DEPLOYMENT_GROUP": "microservices",
        "HAPROXY_0_VHOST": "unconfere-publicsl-c9kql48fmfr4-125691879.ap-southeast-2.elb.amazonaws.com"
    },
    "mem": 128,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0
    }
}